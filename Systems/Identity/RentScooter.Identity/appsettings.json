{
  "AllowedHosts": "*",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3} ({CorrelationId})] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },

  "Main": {
    "MainUrl": "http://localhost:5213" //(IDS port is 5273) 5213
  },

  "Database": {
    "Type": "PostgreSQL",
    "ConnectionString": "Server=localhost;Port=5432;Database=RentScooter;User Id=postgres;Password=Passw0rd;"
  }

