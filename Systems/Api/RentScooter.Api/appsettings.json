{
  "AllowedHosts": "*",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3} ({CorrelationId})] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },

  "Main": {
    "MainUrl": "http://localhost"
  },

  "Identity": {
    "Url": "http://localhost:10001"
  },

  "Database": {
    "Type": "PostgreSQL",
    //"ConnectionString": "Server=localhost;Port=25432;Database=RentScooter;User Id=postgres;Password=Passw0rd;",
    "ConnectionString": "Server=localhost;Port=5432;Database=RentScooter;User Id=postgres;Password=Passw0rd;" //отладка через api в ws-3 на 1:28:44
  },

  "Swagger": {
    "Enabled": true,
    "OAuthClientId": "",
    "OAuthClientSecret": ""
  },

  "ApiSpecial": {
    "HelloMessage": "Hello, RentScooter!"
  },

  "Cache": {
    "Uri": "http://localhost:6379,allowAdmin=true,abortConnect=false",
    "CacheLifeTime": 1
  },

    "RabbitMq": {
      "Uri": "amqp://host.docker.internal:5672",
      "UserName": "guest",
      "Password": "guest"
    }

}

